{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "doctrine/dbal": "^2.8",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "laravolt/avatar": "^2.0",
        "maatwebsite/excel": "~2.1.0",
        "nexmo/laravel": "^1.1",
        "olifolkerd/convertor": "dev-master",
        "owen-it/laravel-auditing": "^7.0",
        "pragmarx/countries": "^0.5.8",
        "propaganistas/laravel-phone": "^4.0",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-backup": "^5.9",
        "spatie/laravel-medialibrary": "^7.0.0",
        "symfony/polyfill-iconv": "^1.10"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
          "files": [
        "app/helpers.php"
    ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/olifolkerd/convertor"
        }
    ]
}
